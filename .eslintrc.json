{
    "env": {
        "es6": true,
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base", 
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "pretier/prettier": 0,
        "no-console": "off",
        "import/prefer-default-export": "off"
    }
}

